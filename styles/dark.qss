/*
* Dark stylesheet for TeamSpeak 3 client
*
* Copyright (c) 2017 random-host.com
*/

/* Main colors for most elements */
QMainWindow,
QWidget,
QDialog,
TextBrowser,
QAbstractItemView,
QTextEdit,
QSplitter {
    background: #36393E;
    color: #dcddde;
}

/* Box that groups elements together (Example: Tools -> Options -> Application -> Server */
QGroupBox {
    border: none;
    border-bottom: 1px solid #484C52;
    padding-top: 13px;
    margin-top: 5px;
    font-weight: bold;
}
QGroupBox::title {
    color: #B9BBBE;
    font-weight: bold;
    padding: 0;
}

/* Options dialog title and description */
QWidget#HeadlineLabel,
ProfileListWidget {
    background-color: #292B2F;
    border: 1px solid #202225;
    border-radius: 3px;
}
QLabel#OptionsHeadlineTitleLabel {
    background: none;
    color: #5F6164;
    font-weight: bold;
}
QLabel#OptionsHeadlineDescriptionLabel {
    background: none;
    color: #dcddde;
}

/* Server tabs widget and chat widget */
QWidget#MainWindowServerTabsWidget,
QWidget#MainWindowChatWidget {
    background-color: transparent;
}
QWidget::pane {
    border: none;
}

/* Scroll area widgets (no idea where these are used in the client yet) */
QWidget#scrollAreaWidgetContents,
QWidget#klappWidget_1,
QWidget#klappWidget_2,
QWidget#klappWidget_3,
QWidget#klappWidget_4,
QWidget#klappWidget_5,
QWidget#klappWidget_6,
QWidget#klappWidget_7 {
    background-color: #2F3136;
    padding: 0;
    border: none;
}

/* Titles for "Bookmarks", "Identities" and "myTeamSpeak" login  */
QLabel#bookmarksBanner,
QLabel#identitiesBanner,
QLabel#accountLoginBanner,
QLabel#accountCreateBanner {
    background-color: #292B2F;
    border: 1px solid #202225;
    border-radius: 3px;
    color: #5F6164;
    font-weight: bold;
    padding-left: 6px;
    qproperty-minimumSize: 0px 32px;
}
QLabel#accountLoginBanner,
QLabel#accountCreateBanner {
    qproperty-minimumSize: 420px 32px;
}

/* Statusbar */
QStatusBar {
    background-color: transparent;
}
/* Remove status bar deviders */
QStatusBar::item {
    border-right: none;
}

/* News ticker in the status bar */
QWidget#NewsBanner {
    color: #dcddde;
    background-color: transparent;
    border: none;
    padding-left: 5px;
    font-style: normal;
}
/* NOTE: font-style won't work until TeamSpeak Systems stops hard-coding markup into their labels */
QLabel#NewsLeftLabel {
    padding-right: 6px;
    font-weight: bold;
    font-style: normal;
}

/* QGroupBox */
QGroupBox#filetransferGroupBox,
QGroupBox#groupOptionsGroupBox,
QGroupBox#adminServerGroupBox,
QGroupBox#manageGroupsGroupBox,
QGroupBox#assignRemoveMemberGroupBox,
QGroupBox#adminClientsGroupBox,
QGroupBox#manageChannelsGroupBox,
QGroupBox#basicsGroupBox {
    background-color: #2F3136;
    border: 1px solid #202225;
    border-radius: 3px;
    margin-top: 2ex;
}
QGroupBox#filetransferGroupBox::title,
QGroupBox#groupOptionsGroupBox::title,
QGroupBox#adminServerGroupBox::title,
QGroupBox#manageGroupsGroupBox::title,
QGroupBox#assignRemoveMemberGroupBox::title,
QGroupBox#adminClientsGroupBox::title,
QGroupBox#manageChannelsGroupBox::title,
QGroupBox#basicsGroupBox::title {
    subcontrol-origin:margin;
    padding:0 5px;
    margin-left: 8px;
    left:11px;
}
/* Invisible splitter handles */
QSplitter::handle {
    background-color: transparent;
}
QSplitter::handle:horizontal {
    width: 2px;
}
QSplitter::handle:vertical {
    height: 2px;
}

/* Multi line text inputs */
QTextEdit
{
    border: 1px solid #202225;
    background: #303338;
    padding: 6px;
    border-radius: 3px;
}

/* Single line text inputs */
QLineEdit {
    border:1px solid #222427;
    border-radius: 3px;
    background: #303338;
    padding: 6px;
}

/* Chat input got it's own class */
ChatLineEdit {
    border: none;
    background: #484B51;
    border-radius: 3px;
    padding: 1px;
}

/* Inline text input for changing nicknames */
QExpandingLineEdit {
    border:1px solid #222427;
    border-radius: 3px;
    background: #303338;
    padding: 0px;
}

/*
 * Push buttons (e.g. "OK", "Cancel", "Apply")
 *
 * These are used in a thousand places even where it doesn't make much sense
 * such as Tools -> Options -> Addons.
 */
QPushButton {
    background-color: #7289DA;
    border: none;
    border-radius: 3px;
    padding-left: 14px; padding-right: 14px;
    padding-top: 8px; padding-bottom: 8px;
    font-weight: bold;
    color: #ffffff;
}
QPushButton:hover {
    background: #677BC4;
    color: #ffffff;
}
QPushButton:pressed {
    background: #677BC4;
    color: #ffffff;
}
QPushButton:disabled {
    border: none;
    border-radius: 3px;
    background: #484B51;
    color: #777777;
}
QPushButton:checked {
    background-color: #677BC4;
}

QPushButton:checked:hover {
    background-color: #677BC4;
}

/* MenuBar */
QMenuBar {
    background: #202225;
    spacing: 2px;
    color: #dddddd;
}
QMenuBar::item {
    background: #202225;
    color: #dddddd;
}
QMenuBar::item:selected {
    background: #42464D;
    color: #ffffff;
}
QMenuBar::item:pressed {
    background: #42464D;
    color: #ffffff;
}
QMenu {
    background: #2F3136;
    color: #dddddd;
}
QMenu::item {
    background: transparent;
}
QMenu::item:disabled {
    color: #43454B;
}
QMenu::item:selected {
    background: #36393F;
    color: #ffffff;
}
QMenu::separator {
    height:1px;
    margin-bottom: 2px;
    margin-top: 2px;
    background: #43454B;
}
/*
 * QMenuIndicator
 *
 * These are used to indicate "checked" options in context menus but cause
 * problems with the "Channel Commander" and "Priority Speaker" options.
 */
/*
QMenu::indicator {
	background: #42464D;
}
QMenu::indicator:non-exclusive:checked {
	background: #42464D;
}
*/
/* Combobox (drop-down fields) */
QComboBox {
    border:1px solid #222427;
    border-radius: 3px;
    background: #303338;
    color: #CCCCCC;
    padding: 6px;
    selection-background-color: #26282C;
    font-size: 12px;
}
QComboBox::drop-down {
    background: #303338;
    border: none;
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    width: 15px;
    color: #ffffff;
    image: url("styles:dark/arrow_bold_down.png");
}
QComboBox::drop-down:disabled {
    image: url("styles:dark/arrow_bold_down_disabled.png");
}
QComboBox:!editable:on,
QComboBox::drop-down:editable:on {
    background: #303338;
}
QComboBox:on {
    background: #26282C;
}
QComboBox QAbstractItemView {
    background: #303237;
    border: none;
    selection-background-color: #26282C;
}
QComboBox#utcLocalComboBox {
    padding-left: 14px;
    padding-right: 7px;
}

/* Listview (e.g. "Permissions", "Server Groups", etc.) */
QListView {
    background: #2F3136;
    color:#dcddde;
    selection-background-color: #26282C;
    selection-color: #ffffff;
    show-decoration-selected: 1;
    border: 1px solid #222427;
    border-radius: 3px;
}
QListView::item:hover {
    background: #42464D;
    border: none;
    border-radius: 0;
}
QListView::item:selected:active {
    background: #42464D;
    color: #ffffff;
    border: none;
}
QListView::item:selected:!active {
    background: #42464D;
    color: #ffffff;
    border: none;
}
/* Avoid blue default font colour of unselected items (Example: Permissions -> Server Groups) */
QListView#groupsListView::item:!selected {
  color: rgb(216,216,216);
}
QListWidget#identitiesListWidget {
    background: #2F3136;
    color:#dddddd;
}
QListView#fileListView::item:hover {
    background: transparent;
}
QListView#fileListView::item:selected {
    background: transparent;
}

/* Toolbar (main toolbar with buttons and volume slider) */
QToolBar {
    border: transparent;
    background: #2F3136;
    spacing: 5px;
}
/* ToolButton (e.g. "Toggle Away Status", "Mute Microphone", etc.) */
QToolButton {
    border: none;
    border-radius: 3px;
    background-color: #3C3F44;
    border-radius: 3px;
    font-weight: bold;
    color: #ffffff;
}
QToolButton:pressed {
    border: none;
    border-radius: 3px;
    background: #677BC4;
    color: #ffffff;
}
QToolButton:hover {
    border: none;
    border-radius: 3px;
    background: #677BC4;
    color: #ffffff;
}
QToolButton:checked,
QToolButton#serverQueryAdvancedButton:checked {
    border: none;
    border-radius: 3px;
    background: #7289DA;
    color: #dddddd;
}
QToolButton[popupMode="1"] {
    border: none;
    border-radius: 3px;
    padding-right: 15px;
}
QToolButton[popupMode="1"]:hover {
    border: none;
    border-radius: 3px;
    padding-right: 15px;
}
QToolButton[popupMode="1"]:checked {
    border: none;
    background: #7289DA;
    color: #dddddd;
    border-radius: 3px;
    padding-right: 15px;
}
QToolButton::menu-button {
    border: none;
    border-top-right-radius: 3px;
    border-bottom-right-radius: none;
    width: 12px;
}
QToolButton::menu-button:hover {
    border: none;
    border-radius: 3px;
}
QToolButton#newBookmarkButton,
QToolButton#newBookmarkFolderButton,
QToolButton#removeBookmarkButton {
    background: none;
    border: 1px solid transparent;
}
QToolButton#newBookmarkButton:hover,
QToolButton#newBookmarkFolderButton:hover,
QToolButton#removeBookmarkButton:hover {
    background: #25262A;
    border: none;
    border-radius: 3px;
}
QToolButton#serverQueryRunButton,
QToolButton#serverQuerySingleStepsButton,
QToolButton#serverQueryStopButton,
QToolButton#serverQueryRestartButton,
QToolButton#serverQueryAdvancedButton,
QToolButton#serverQueryClearHighlightButton,
QToolButton#newSQScriptButton,

    color: #dddddd;
}
QToolBoxButton#qt_toolbox_toolboxbutton {
    background-color: #33363C;
    border: none;
    border-radius: 3px;
}

/* TabBar (e.g. server tabs, chat tabs, permission group tabs, etc.) */
QTabWidget::tab-bar {
    alignment: left;
}
QTabWidget#permTabWidget::pane {
    border: none;
}
QTabBar {
    background-color: transparent;
    border: none;
}
QTabBar::tab {
    background: #3C3F44;
    border: none;
    color: #dddddd;
    min-width: 8ex;
    padding: 6px;
}
QTabBar::tab:selected, QTabBar::tab:hover {
    background: #2F3136;
    color: #ffffff;
}

QTabBar::tab:selected:top {
    border-top: 3px solid #7289DA;
    margin-top: 0;
}
QTabBar::tab:selected:bottom {
    border-bottom: 3px solid #7289DA;
    margin-bottom: 0;
}
QTabBar::tab:!selected:top {
    border-top: 3px solid #36393E;
    margin-top: 0;
}
QTabBar::tab:!selected:bottom {
    border-bottom: 5px solid #36393E;
    margin-bottom: 0;
}

/* SpinBox (input fields with "up" and "down" buttons) */
QSpinBox,
QDoubleSpinBox,
QDoubleSpinBox#delayPTTSpinBox,
QSpinBox#uploadBWSpinBox,
QSpinBox#downloadBWSpinBox,
QSpinBox#reloadChannelChatsSpinBox,
QSpinBox#reloadClientChatsSpinBox,
QSpinBox#uploadSpeedSpinBox,
QSpinBox#downloadSpeedSpinBox {
    background: #303338;
    border:1px solid #222427;
    border-radius: 3px;
    padding: 6px;
    color: #CCCCCC;
    selection-background-color: #26282C;
}
QSpinBox::disabled,
QDoubleSpinBox#delayPTTSpinBox::disabled,
QSpinBox#uploadBWSpinBox::disabled,
QSpinBox#downloadBWSpinBox::disabled,
QSpinBox#reloadChannelChatsSpinBox::disabled,
QSpinBox#reloadClientChatsSpinBox::disabled,
QSpinBox#uploadSpeedSpinBox::disabled,
QSpinBox#downloadSpeedSpinBox::disabled {
    background:  #303338;
    border:1px solid #222427;
    border-top-right-radius: 3px;
    border-bottom-left-radius: 3px;
    padding: 6px;
    color: #43454B;
    selection-background-color: none;
}
QSpinBox::up-button,QDoubleSpinBox::up-button,
QDoubleSpinBox#delayPTTSpinBox::up-button {
    background: transparent;
    border: none;
    border-top-right-radius: 3px;
    width: 15px;
    color: #ffffff;
}
QSpinBox::up-button:hover,
QDoubleSpinBox#delayPTTSpinBox::up-button:hover {
    background: transparent;
}
QSpinBox::up-button:pressed,
QDoubleSpinBox#delayPTTSpinBox::up-button:pressed {
    background: transparent;
}
QSpinBox::up-arrow,
QDoubleSpinBox::up-arrow {
    image:url("styles:dark/arrow_bold_up.png");
    background-position: center center;
    background-repeat: no-repeat;
}
QSpinBox::up-arrow:disabled,
QSpinBox::up-arrow:off,
QDoubleSpinBox#delayPTTSpinBox::up-arrow:disabled,
QDoubleSpinBox#delayPTTSpinBox::up-arrow:off {
    image: url("styles:dark/arrow_bold_up_disabled.png");
}
QSpinBox::down-button,
QDoubleSpinBox::down-button,
QDoubleSpinBox#delayPTTSpinBox::down-button {
    background: transparent;
    border:none;
    border-bottom-right-radius: 3px;
    width: 15px;
}
QSpinBox::down-button:hover,
QDoubleSpinBox#delayPTTSpinBox::down-button:hover {
    background: #303338;
}
QSpinBox::down-button:pressed,
QDoubleSpinBox#delayPTTSpinBox::down-button:pressed {
    background: #303338;
}
QSpinBox::down-arrow,
QDoubleSpinBox::down-arrow {
    image: url("styles:dark/arrow_bold_down.png");
    background-position: center center;
    background-repeat: no-repeat;
}
QSpinBox::down-arrow:disabled,
QSpinBox::down-arrow:off,
QDoubleSpinBox#delayPTTSpinBox::down-arrow:disabled,
QDoubleSpinBox#delayPTTSpinBox::down-arrow:off {
    image: url("styles:dark/arrow_bold_down_disabled.png");
}
/* Tree View (e.g. channel tree) */
QTreeView {
    alternate-background-color: transparent;
    show-decoration-selected: 0;
    border: 1px solid #202225;
    border-radius: 3px;
}
TreeView {
    background: #2F3136;
    border: none;
    border-radius: 0;
    color:#dddddd;
}
QTreeView::item {
    height: 16px;
    padding: 1px;
    border: 0px;
}
QTreeView::item:hover {
    background: #36393F;
    color: #ffffff;
}
QTreeView::item:selected:active {
    background: #42464D;
    color: #ffffff;
}
QTreeView::item:selected:!active {
    background: #42464D;
    color: #ffffff;
}
QTreeView::branch:hover {
    background: #25262A;
}
QTreeView::branch:selected {
    background: #42464D;
}
QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings {
    border-image: none;
    image: url(styles:dark/arrow_open.png);
}
QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings  {
    border-image: none;
    image: url(styles:dark/arrow_close.png);
}
QTreeView#eventsTreeView::indicator:checked,
QTreeView#permissionsTreeView::indicator:checked,
QTreeWidget#appsTreeWidget::indicator:checked,
QListWidget#pluginsListWidget::indicator:checked,
QListView#ServerGroupsListView::indicator:checked,
QTreeWidget#clientsTreeWidget::indicator:checked,
QGroupBox#advancedGroupBox::indicator:checked {
    background-color: transparent;
    border: none;
    border-radius: 3px;
    image: url(styles:dark/checkbox_tree_enabled.png);
}
QTreeView#eventsTreeView::indicator:checked:hover,
QTreeView#permissionsTreeView::indicator:checked:hover,
QTreeWidget#appsTreeWidget::indicator:checked:hover,
QListWidget#pluginsListWidget::indicator:checked:hover,
QListView#ServerGroupsListView::indicator:checked:hover,
QTreeWidget#clientsTreeWidget::indicator:checked:hover,
QGroupBox#advancedGroupBox::indicator:checked:hover {
    background: transparent;
}
QTreeView#eventsTreeView::indicator:unchecked,
QTreeView#permissionsTreeView::indicator:unchecked,
QTreeWidget#appsTreeWidget::indicator:unchecked,
QListWidget#pluginsListWidget::indicator:unchecked,
QListView#ServerGroupsListView::indicator:unchecked,
QTreeWidget#clientsTreeWidget::indicator:unchecked,
QGroupBox#advancedGroupBox::indicator:unchecked {
    background-color: transparent;
    border: none;
    border-radius: 3px;
    image: url(styles:dark/checkbox_tree_disabled.png);
}
QTreeView#eventsTreeView::indicator:unchecked:hover,
QTreeView#permissionsTreeView::indicator:unchecked:hover,
QTreeWidget#appsTreeWidget::indicator:unchecked:hover,
QListWidget#pluginsListWidget::indicator:unchecked:hover,
QListView#ServerGroupsListView::indicator:unchecked:hover,
QTreeWidget#clientsTreeWidget::indicator:unchecked:hover,
QGroupBox#advancedGroupBox::indicator:unchecked:hover {
    background: transparent;
}
QTreeWidget#clientsTreeWidget {
    background: transparent;
}

/* Scroll bars */
QScrollBar {
    border: 1px solid transparent;
    border-radius: 5px;
    background: #2F3136;
    margin: 1px;
}
QScrollBar:vertical {
    width: 12px;
}
QScrollBar:horizontal {
    height: 12px;
}
QScrollBar::handle {
    border: 1px solid transparent;
    background: #1E2124;
    border-radius: 4px;
}
QScrollBar::handle:vertical {
    min-height: 20px;
}
QScrollBar::handle:horizontal {
    min-width: 20px;
}
QScrollBar::up-arrow:vertical {
    background: url("styles:dark/arrow_bold_up.png");
    background-repeat: none;
    left: 3px;
    top: 5px;
}
QScrollBar::down-arrow:vertical {
    background: url("styles:dark/arrow_bold_down.png");
    background-repeat: none;
    left: 3px;
    top: 5px;
}
QScrollBar::left-arrow:horizontal {
    background: url("styles:dark/arrow_bold_left.png");
    background-repeat: none;
    left: 5px;
    top: 3px;
}
QScrollBar::right-arrow:horizontal {
    background: url("styles:dark/arrow_bold_right.png");
    background-repeat: none;
    left: 5px;
    top: 3px;
}
QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical,
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    background: none;
}
QScrollBar::add-line:vertical,
QScrollBar::add-line:horizontal,
QScrollBar::sub-line:vertical,
QScrollBar::sub-line:horizontal {
    width: 0px;
    height: 0px;
}

/* HeaderView */
QHeaderView::section {
    background: #26282C;
    color: #dddddd;
    height: 25px;
    padding-left: 5px;
    border: none;
    border-bottom: 1px solid #484B51;
    border-right: 1px solid #484B51;
}
QHeaderView::section:last {
    border-right: none;
}

/* TableView (e.g. hotkeys, notifications) */
QTableView {
    alternate-background-color: #2F3237;
    selection-background-color: #26282C;
    selection-color: #ffffff;
    qproperty-alternatingRowColors: 1;
    border: 1px solid #484B51;
    gridline-color: #484B51;
}
QTableView::item {
    border: 1px solid transparent;
}
QTableView::item:hover {
    background: #36393F;
    border: 1px solid transparent;
    color: #ffffff;
}
QTableView::item:selected:active {
    background: #42464D;
    color: #ffffff;
    border: 1px solid transparent;
}
QTableView::item:selected:!active {
    background: #42464D;
    border: 1px solid transparent;
}

/*QSlider (e.g. volume slider, etc.) */
QSlider {
    background: none;
    padding-top: 6px;
    padding-bottom: 6px;
}
QSlider::groove:horizontal {
    background: #7289DA;
    border: 1px solid transparent;
    border-radius: 3px;
    padding-right: 5px;
    padding-left: 5px;
    height: 7px;
}
QSlider::sub-page:horizontal { /*left side*/
    background: #7289DA;
    border-bottom: none;
    border-left: none;
    border-top: none;
    border-radius: 3px;
}
QSlider::add-page:horizontal { /*right side*/
    background: #4F545C;
    border-bottom: none;
    border-right: none;
    border-top: none;
    border-radius: 3px;
}
QSlider::handle:horizontal {
    background: #ffffff;
    border-bottom: none;
    border-right: none;
    border-left: none;
    border-top: none;
    border-radius: 3px;
    margin: -8px 0;
    width: 9px;
}

QSlider::groove:vertical {
    background: #7289DA;
    border: 1px solid transparent;
    border-radius: 3px;
    padding-top: 5px;
    padding-bottom: 5px;
    width: 7px;
}
QSlider::sub-page:vertical { /*top side*/
    background: #7289DA;
    border-bottom: none;
    border-left: none;
    border-top: none;
    border-radius: 3px;
}
QSlider::add-page:vertical { /*bottom side*/
    background: #4F545C;
    border-bottom: none;
    border-right: none;
    border-top: none;
    border-radius: 3px;
}
QSlider::handle:vertical {
    background: #ffffff;
    border-bottom: none;
    border-right: none;
    border-left: none;
    border-top: none;
    border-radius: 3px;
    margin: 0 -8px;
    height: 9px;
}

/* Excluding background for specific elements */
QLabel,
QCheckBox,
QRadioButton,
QGroupBox,
QSlider,
QFrame#bookmarksTopFrame,
QWidget#advancedWidget,
QWidget#statusChangeWidget,
QWidget#connectToWidget,
QWidget#switchToWidget,
QWidget#disableWidget,
QWidget#activateWidget,
QWidget#recordingWidget,
ImprovedTabBarCloseButton,
QWidget#avatarWidget,
QWidget#ToolbarStretcher,
QStackedWidget {
    background: none;
}
QWidget#ConnectionInfoDialog,
FriendFoeManager,
ServerLogView,
ClientLogView,
FileBrowser,
BookmarksManager,
QWidget#UrlCatcherViewer,
QWidget#ServerConnectionInfoDialog,
LogDisplay,
QDialog#BanDialog,
QWidget#addPropertiesWidget,
EmoticonsDisplay,
QWidget#OfflineMessagesDialog,
QWidget#sound3Dwidget,
UserVolumeWidget,
QWidget#sound3DTestWidget,
QWidget#CustomizeToolbar {
    background: #2F3136;
}
QWidget#ServerQueryView {
    background: #33363C;
}
QTextEdit#pluginDescTextEdit,
QTextEdit#descriptionTextEdit,
QTextEdit#vserverWelcomeTextEdit,
QTextEdit#changeLogTextEdit {
    border:1px solid #222427;
    background: #303338;
    border-radius: 3px;
    padding: 6px;
}
MainWindowServerTabsWidget,
MainWindowChatWidget {
    background: transparent;
}

/* Disabled elements */
QLineEdit:disabled,
QComboBox:disabled,
QSpinBox:disabled {
    border:1px solid #222427;
    border-radius: 3px;
    background: #303338;
    color: #43454B;
}
QCheckBox:disabled,
QRadioButton:disabled,
QTextEdit:disabled {
    color: #43454B;
}

QTextEdit:disabled {
    background: #303338;
    color: #43454B;
}

QTextEdit#pluginDescTextEdit:disabled,
QTextEdit#descriptionTextEdit:disabled,
QTextEdit#vserverWelcomeTextEdit:disabled,
QTextEdit#changeLogTextEdit:disabled {
    border:1px solid #222427;
    border-radius: 3px;
    background: #303338;
}

/* Use iOS style "Switches" instead of checkboxes and radio buttons */
QCheckBox::indicator:checked {
    height: 20px;
    width: 35px;
    background: #7289DA;
    border: none;
    border-radius: 10px;
    image: url(styles:dark/checkbox_enabled.png);
}
QCheckBox::indicator:unchecked {
    height: 20px;
    width: 35px;
    background: #72767D;
    border: none;
    border-radius: 10px;
    image: url(styles:dark/checkbox_disabled.png);
}

QCheckBox::indicator:checked:hover {
    background: #7289DA;
}
QCheckBox::indicator:unchecked:hover {
    background: #72767D;
}
QCheckBox::indicator:disabled:checked {
    background: #484B51;
    image: url(styles:dark/checkbox_inactive_enabled.png);
}
QCheckBox::indicator:disabled:unchecked {
    background: #484B51;
    image: url(styles:dark/checkbox_inactive_disabled.png);
}
QRadioButton {
    border: 1px solid #222427;
    border-radius: 3px;
    background: #303338;
    color: #CCCCCC;
    padding: 3px 4px;
    selection-background-color: #26282C;
    font-size: 12px;
}
QRadioButton::indicator {
    background-color: transparent;
    border: none;
    border-radius: 3px;
    padding-right: 5px;
}
QRadioButton:checked {
    background: #7289DA;
    color: #FFFFFF;
}
QRadioButton:disabled:checked {
    background: #484B51;
    color: #666666;
}
QRadioButton::indicator:checked {
    image: url(styles:dark/checkbox_tree_enabled.png);
}
QRadioButton::indicator::unchecked {
    image: url(styles:dark/checkbox_tree_disabled.png);
}
QRadioButton::indicator:disabled:checked {
    image: url(styles:dark/checkbox_tree_inacitve_enabled.png);
}
QRadioButton::indicator:disabled:unchecked {
    image: url(styles:dark/checkbox_tree_inactive_disabled.png);
}


/* Remove window resize grip */
QSizeGrip {
    background-color: transparent;
    width: 0px;
    height: 0px;
}

/* Logo in top-right corner of Infoframe window*/
InfoFrame {
    border: 0;
    border-radius: 0;
    background: url('styles:dark/info.png') #2F3136;
    background-repeat: none;
    background-position: top right;
    background-attachment: fixed;
}
/* Logo in bottom-right corner of ChatTab window*/
QTextEdit#ChatTab {
    border: 0;
    border-radius: 0;
    background: url('styles:dark/chat.png') #2F3136;
    background-repeat: none;
    background-position: bottom right;
    background-attachment: fixed;
}

/* Wizard */
QWizard QWidget {
    background: #2F3136;
}
QWizard QLabel {
    background: transparent;
}

/* Capture Hotkey dialog */
QLabel#RecorderLabel {
    border: 1px solid #6f80aa;
    background: #2F3136;
}

/* Tool Tips */
QToolTip {
    border:1px solid #222427;
    border-radius: 3px;
    background: #36393E;
    color:#dddddd;
    padding: 6px;
}

/* Poke dialog */
PokeDialog QScrollBar:vertical
{
    width: 0px;
}
PokeDialog QTextEdit
{
    border: none;
    background: transparent;
    padding: 0;
    margin: 0;
    border-radius: 0;
}

/*
 * Attempt to fix the "Recipients" field in "New Offline Message" dialog
 *
 * That dialog is not even a dialog and consists of so many nested elements
 * that I couldn't figure out a proper way to style it yet.
 */
OfflineMessagesNew QScrollArea {
    border: none;
    background: #ff0000;
    margin: 0;
}
OfflineMessagesNew QScrollArea QWidget EscLineEdit {
    border: 1px solid #202225;
    background: #303338;
    padding: 0;
    border-radius: 3px;
    height: 30px !important;
    max-height: 30px !important;
}